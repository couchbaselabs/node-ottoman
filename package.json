{
  "description": "ODM for Couchbase Node.js driver.",
  "keywords": [
    "couchbase",
    "odm",
    "nosql",
    "json",
    "document"
  ],
  "main": "./lib/ottoman",
  "license": "Apache2",
  "name": "ottoman",
  "scripts": {
    "clean": "rm -rf logs",
    "format": "prettier --config ./.prettierrc  --write",
    "format:js": "prettier --config ./.prettierrc --write './**/*.js'",
    "packages": "npm ci",
    "lint": "npm run lint:js .",
    "lint:quiet": "npm run -s lint:js:quiet .",
    "lint:verbose": "npm run -s lint:js:verbose .",
    "lint:js": "eslint --fix .",
    "lint:js:quiet": "eslint --fix --quiet --cache .",
    "lint:js:verbose": "eslint --fix --debug .",
    "lint:js:check": "eslint --print-config . | eslint-config-prettier-check",
    "test": "eslint ./lib && istanbul cover ./node_modules/mocha/bin/_mocha -- test/*.test.js",
    "test:int": "jest --config ./integration-tests/jest.config.json",
    "test:unit": "jest --config ./jest.config.json",
    "start": "nodemon server.js"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.9.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "ink-docstrap": "^1.3.2",
    "jsonpath": "^1.0.0",
    "lodash": "^4.15.0",
    "lodash-deep": "^2.0.0",
    "prettier": "^1.18.2",
    "uuid": "^3.3.2"
  },
  "optionalDependencies": {
    "couchbase": "~2.6.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/couchbaselabs/node-ottoman.git"
  },
  "version": "1.0.5"
}
